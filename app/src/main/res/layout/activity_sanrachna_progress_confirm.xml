                        <TextView
                            android:layout_marginTop="4dp"
                            android:id="@+id/tv_title_phase3"
                            android:textColor="#fff"
                            android:textStyle="bold"
                            android:textAlignment="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Stage 3 (कार्य पूरा होने के बाद)"
                            android:layout_marginBottom="8dp"/>

                        <ImageView
                            android:id="@+id/img_phase3"
                            android:padding="4dp"
                            android:layout_width="28dp"
                            android:layout_height="28dp"
                            android:src="@drawable/checked"
                            android:layout_alignParentEnd="true"/>
                    </RelativeLayout>

                    <LinearLayout
                        android:id="@+id/ll_entry_phase3"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="5dp">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="अभ्युक्ति "
                            android:textColor="#000000"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/et_remark_phase3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/edittext_style"
                            android:hint="अभ्युक्ति डाले"
                            android:inputType="textCapWords"
                            android:maxLength="30"
                            android:padding="10dp"
                            android:textColor="#000000" />

                        <LinearLayout
                            android:layout_marginTop="8dp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center_horizontal|center_vertical">

                            <Button
                                android:id="@+id/btn_phase3"
                                android:layout_width="match_parent"
                                android:layout_height="40dp"
                                android:background="@drawable/greenbuttonshape"
                                android:text="सेभ करें"
                                android:textColor="#FFFFFF" />
                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>


        </ScrollView>

    </LinearLayout>

</RelativeLayout>